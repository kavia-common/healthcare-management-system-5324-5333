# Backend environment configuration

# Required: Full MongoDB connection string
# Example for local: mongodb://localhost:27017
# Example with auth: mongodb+srv://user:pass@cluster.mongodb.net/?retryWrites=true&w=majority
MONGODB_URL=mongodb://localhost:27017

# Required: Database name to use
MONGODB_DB=healthcare_db

# Required: Secret for signing JWTs (use a strong, random value in production)
JWT_SECRET=change_this_secret_in_production

# Optional: JWT algorithm (default: HS256)
JWT_ALGORITHM=HS256

# Optional: Access token expiry in minutes (default: 60)
ACCESS_TOKEN_EXPIRE_MINUTES=60
